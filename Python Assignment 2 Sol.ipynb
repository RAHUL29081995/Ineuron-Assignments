   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "*\n",
      "**\n",
      "***\n",
      "****\n",
      "*****\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n"
     ]
    }
   ],
   "source": [
    "a = \"*\"\n",
    "for i in range(1,6):\n",
    "    print(i * \"*\")\n",
    "    if i == 5:\n",
    "        for j in range(4,0,-1):\n",
    "            print(j * \"*\")\n"
